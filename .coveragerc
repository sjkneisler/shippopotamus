[run]
source = tools
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */mcp_bridge_simple.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    
[html]
directory = htmlcov